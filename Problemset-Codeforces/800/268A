#include <iostream>
using namespace std;

int main(){
    unsigned short n,cont=0;
    cin >> n;
    unsigned short h[n],g[n];
    for(int i = 0;i<n;i++){
        cin >> h[i];
        cin >> g[i];
    }
    for(int i = 0;i<n;i++){
        for(int j=0;j<n;j++){
            if(h[i]==g[j]){
                cont++;
            }
        }
    }
    cout << cont;
    return 0;
}
