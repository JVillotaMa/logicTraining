#include <iostream>
using namespace std;

int main()
{
    uint64_t n;
    int cont=0;
    cin >> n;
    while(n!=0){
        if(n%10==7 || n%10==4){
            cont++;
        }
        n/=10;
    }
    if(cont==4 || cont==7){
        cout << "YES";
    }else{
        cout << "NO";
    }
    return 0;
}
