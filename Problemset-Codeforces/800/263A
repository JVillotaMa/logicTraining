#include <iostream>7
#include <cmath>
using namespace std;

int main(){
    int cont,result,input=0;
    for(int i = 0; i<5;i++){
        for(int j=0; j<5;j++){
            cin >> input;
            if(input==1){
                result=abs(j-2)+abs(i-2);
            }
        }
    }
    cout << result;

    return 0;
}
