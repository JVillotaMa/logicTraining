#include <iostream>
using namespace std;

int main(){
    string s;
    unsigned long cont=0,n;
    cin >> n;
    for(unsigned long i=0;i<n;i++){
        cin >> s;
        switch (s[0])
        {
        case 'T':
            cont+=4;
            break;
        case 'C':
            cont+=6;
            break;
        case 'O':
            cont+=8;
            break;
        case 'D':
            cont+=12;
            break;
        case 'I':
            cont+=20;
            break;
        }
    }
    cout << cont;
    return 0;
}
